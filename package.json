{
  "name": "draftpickit",
  "version": "1.1.0",
  "description": "",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm run start-message",
    "start:with-tests": "concurrently -k -r -s first \"npm run test:watch\" \"npm run open:src\" \"npm run lint:watch\"",
    "start:no-tests": "concurrently -k -r -s first \"npm run open:src\" \"npm run lint:watch\"",
    "dev": "concurrently -k -r -s first \"npm run open:src\" \"npm run lint:watch\"",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run lint && npm run clean-dist",
    "build": "babel-node tools/build.js",
    "test": "jest",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watch",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "Maksym Fedan && Yulia Pilat",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-react-transform": "3.0.0",
    "babel-preset-stage-2": "6.24.1",
    "draft-js": "0.10.5",
    "filepond-plugin-file-rename": "1.0.1",
    "filepond-plugin-file-validate-type": "1.1.0",
    "filepond-plugin-image-preview": "1.1.0",
    "firebase": "5.3.0",
    "gsap": "2.0.1",
    "history": "4.6.0",
    "loaders.css": "0.1.2",
    "node-sass": "4.9.2",
    "nprogress": "0.2.0",
    "object-assign": "4.1.0",
    "react": "16.4.2",
    "react-awesome-button": "4.2.1",
    "react-bootstrap": "0.32.1",
    "react-dom": "16.4.2",
    "react-filepond": "2.0.7",
    "react-flip-move": "3.0.2",
    "react-loaders": "3.0.1",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-drilldown": "3.0.2",
    "react-router-redux": "5.0.0-alpha.8",
    "react-star-rating-component": "1.4.1",
