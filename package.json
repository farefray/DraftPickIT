    "enzyme": "3.0.0",
    "enzyme-adapter-react-16": "1.0.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-react": "7.8.2",
    "eslint-watch": "3.1.5",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "hard-source-webpack-plugin": "0.6.4",
