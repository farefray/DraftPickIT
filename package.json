    "react-view-slider": "3.0.5",
    "redraft": "0.10.1",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-nprogress": "1.0.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "8.5.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "23.0.0",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
