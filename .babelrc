{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 Chrome versions"],
        "node": "current"
      }
    }], "react", "stage-2"
  ],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }]
      ]
    }
  }
}
